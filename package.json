{
  "name": "js-merialize-laria",
  "version": "1.0.0",
  "description": "Pure Javascript Serialized Merkle-Tree Library",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "check-format": "prettier --ignore-path .gitignore --list-different \"**/*.+(js|ts|json)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .gitignore .",
    "validate": "npm run test && npm run check-format && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAFECA-IO/js-Merialize-Laria.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CAFECA-IO/js-Merialize-Laria/issues"
  },
  "homepage": "https://github.com/CAFECA-IO/js-Merialize-Laria#readme",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^4.3.8",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "ts-jest": "^28.0.8"
  },
  "dependencies": {
    "@cafeca/keccak": "^0.8.4",
    "eslint": "^8.22.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  }
}
